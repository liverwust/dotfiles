{
    "workbench.colorTheme": "Solarized Light",
    "files.autoSave": "afterDelay",
    "editor.dragAndDrop": false,
    "editor.emptySelectionClipboard": false,
    "workbench.startupEditor": "newUntitledFile",
    "window.restoreWindows": "none",
    "files.insertFinalNewline": true,
    "vim.cmdLineInitialColon": true,
    "editor.tabSize": 8,
    "editor.insertSpaces": false,
    "vim.handleKeys": {
        "<C-b>": false,
        "<C-d>": true,
        "<C-k>": false,
        "<C-o>": false,
        "<C-s>": false
    },
    "vim.cursorStylePerMode": {
        "insert": "line"
    },
    "[json]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true
    },
    "[powershell]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true
    },
    "[python]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true
    },
    "editor.minimap.enabled": false,
    "editor.acceptSuggestionOnEnter": "off",
    "window.menuBarVisibility": "toggle",
    "workbench.statusBar.visible": true,
    "telemetry.telemetryLevel": "crash",
    "editor.autoClosingQuotes": "never",
    "editor.autoClosingBrackets": "never",
    "trailing-spaces.backgroundColor": "rgba(255,255,0,0.3)",
    "extensions.ignoreRecommendations": true,
    "powershell.powerShellAdditionalExePaths": [
        {
            "versionName": "PowerShell 7.1.3",
            "exePath": "/net2/lww/powershell/7/pwsh"
        }
    ],
    "powershell.powerShellDefaultVersion": "PowerShell 7.1.3",
    "workbench.activityBar.location": "top",
    "git.autofetch": true,
    "git.rebaseWhenSync": true,
    "workbench.editor.enablePreview": false,
    "explorer.autoReveal": false,
    "workbench.preferredLightColorTheme": "Solarized Light",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "notebook.editorOptionsCustomizations": {
        "editor.tabSize": 4,
        "editor.indentSize": 4,
        "editor.insertSpaces": true
    },
    "jupyter.widgetScriptSources": [
        "jsdelivr.com",
        "unpkg.com"
    ],
    "files.trimTrailingWhitespace": true,
    "terminal.integrated.scrollback": 10000,
    "terminal.integrated.macOptionIsMeta": true,
    "editor.wordWrap": "on",
    "window.zoomLevel": 1,
    "remote.SSH.enableAgentForwarding": false,
    "remote.SSH.enableX11Forwarding": false,
    "remote.SSH.useLocalServer": false,
    "remote.SSH.experimental.chat": false,
    "chat.agent.enabled": false,
    "chat.implicitContext.suggestedContext": false,
    "accessibility.verbosity.inlineChat": false,
    "accessibility.verbosity.panelChat": false,
    "accessibility.openChatEditedFiles": false,
    "chat.checkpoints.enabled": false,
    "chat.commandCenter.enabled": false,
    "chat.detectParticipant.enabled": false,
    "chat.editing.confirmEditRequestRemoval": false,
    "chat.editing.confirmEditRequestRetry": false,
    "chat.editRequests": "none",
    "chat.edits2.enabled": false,
    "chat.emptyChatState.enabled": false,
    "chat.extensionTools.enabled": false,
    "chat.mcp.enabled": false,
    "chat.notifyWindowOnConfirmation": false,
    "chat.promptFiles": false,
    "chat.sendElementsToChat.attachCSS": false,
    "chat.sendElementsToChat.attachImages": false,
    "chat.sendElementsToChat.enabled": false,
    "chat.tools.useTreePicker": false,
    "chat.undoRequests.restoreInput": false,
    "chat.useFileStorage": false,
    "inlineChat.accessibleDiffView": "off",
    "inlineChat.holdToSpeech": false,
    "inlineChat.lineNaturalLanguageHint": false,
    "vscode-neovim.neovimExecutablePaths.win32": "C:\\Program Files\\Neovim\\bin\\nvim.exe",
    "remote.SSH.remotePlatform": {
        "*.nasa.gov": "linux"
    }
}
